{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverage": true,
  "coverageDirectory": "../coverage-e2e",
  "coverageThreshold": {
    "global": {
      "functions": 80,
      "lines": 80
    }
  }
}
